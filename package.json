{
  "name": "eecomparefiles",
  "version": "0.0.1",
  "repository": "https://github.com/Juan1ll0/eecomparefiles.git",
  "description": "App to compare and merge two files",
  "main": "app/main.js",
  "scripts": {
    "elm": "elm make app/Main.elm --output build/elm.js || true",
    "elm:watch": "chokidar '**/*.elm' -c 'npm run elm'",
    "start": "electron .",
    "watch": "npm run start & npm run elm:watch & wait"
  },
  "keywords": [
    "electron",
    "merge",
    "elm"
  ],
  "author": "Juan Ignacio Fernandez Diez",
  "license": "ISC",
  "devDependencies": {
    "chokidar": "^1.6.1"
  }
}
